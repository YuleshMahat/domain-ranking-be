
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
   url = env("DATABASE_URL")
}


model Domain {
  id  Int @id @default(autoincrement())
  name  String @unique
  rankings Ranking[]
}

model Ranking {
  id       Int      @id @default(autoincrement())
  rank     Int
  date     DateTime @default(now())

  domainId Int
  domain   Domain   @relation(fields: [domainId], references: [id])
}


